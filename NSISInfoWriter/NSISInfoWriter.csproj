<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;net46</TargetFrameworks>
    <Product>NSISInfoWriter</Product>
    <Description>NSIS Script Writer</Description>
    <Authors>Babaev Evgeniy</Authors>
    <Company />
    <Copyright>Copyright © 2015-2017</Copyright>
    <AssemblyVersion>1.4.0.0</AssemblyVersion>
    <FileVersion>1.4.0.0</FileVersion>
    <PackageProjectUrl>https://github.com/segrived/NSISInfoWriter</PackageProjectUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <RepositoryUrl>https://github.com/segrived/NSISInfoWriter</RepositoryUrl>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <StartupObject>NSISInfoWriter.Program</StartupObject>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="NSISInfoWriter.args.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.1.1-beta" />
    <PackageReference Include="System.IO.Compression" Version="4.3.0" />
  </ItemGroup>
</Project>